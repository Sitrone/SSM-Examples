CREATE DATABASE goo;

CREATE TABLE IF NOT EXISTS `user`
(
    `id` INT UNSIGNED NOT NULL PRIMARY KEY AUTO_INCREMENT,
    `age` INTEGER NOT NULL,
    `name` VARCHAR(64) NOT NULL DEFAULT '',
    `desc` VARCHAR(256)
)ENGINE=InnoDB DEFAULT CHARSET=utf8;

